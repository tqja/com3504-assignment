<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %></title>
    <link rel="icon" type="image/x-icon" href="/images/favicon.ico">
    <link rel="stylesheet" href="stylesheets/style.css">
    <script src="https://kit.fontawesome.com/eff69f9267.js" crossorigin="anonymous"></script>
    <script src="/js/homeHandler.js" defer></script>
    <script src="/js/navHandler.js" defer></script>
</head>
<body>
<%- include("partials/header.ejs"); %>
<main>
    <div class="flex lg:hidden">
        <button id="sidebarBtn" onclick="toggleSidebar()" class="w-full mx-2 px-4 py-2 my-8 bg-gray-200 rounded hover:bg-gray-300 active:bg-gray-400 transition ease-in">Sort/Filter</button>
    </div>
    <div class="flex bg-gray-100">
        <div id="sidebar" class="top-0 left-0 w-full shadow-none p-4 bg-gray-100 hidden lg:block lg:w-80 lg:shadow-2xl">
            <div class="block">
                <label for="sort-by" class="inline-block text-xl w-full pb-3 mb-3 border-b-2 border-gray-300">Sort
                    by</label>
                <select id="sort-by" onchange="sortPlants(this.value)"
                        class="w-full px-4 py-2 mb-6 bg-gray-200 rounded">
                    <option value="dateSeen-desc">Date Seen (Newest First)</option>
                    <option value="dateSeen-asc">Date Seen (Oldest First)</option>
                </select>
            </div>
            <h2 class="inline-block text-xl min-w-full pb-3 mb-3 border-b-2 border-gray-300">Filters</h2>

            <!-- Colour Filter -->
            <label for="colour" class="block text-gray-600 mb-3">Colour</label>
            <select id="colour" class="w-full px-4 py-2 bg-gray-200 rounded">
                <option value="red">Red</option>
                <option value="orange">Orange</option>
                <option value="brown">Brown</option>
                <option value="yellow">Yellow</option>
                <option value="green">Green</option>
                <option value="blue">Blue</option>
                <option value="purple">Purple</option>
                <option value="pink">Pink</option>
                <option value="black">Black</option>
                <option value="grey">Grey</option>
                <option value="white">White</option>
            </select>

            <!-- Soil Filter -->
            <label for="soil" class="block text-gray-600 pt-3 mb-3 mt-3 border-t-2 border-gray-300">Soil Type</label>
            <select id="soil" class="w-full px-4 py-2 bg-gray-200 rounded">
                <option value="red">Any</option>
                <option value="orange">None</option>
                <option value="brown">Chalk</option>
                <option value="yellow">Clay</option>
                <option value="green">Loam</option>
                <option value="blue">Sand</option>
            </select>

            <!-- Sunlight Filter -->
            <h3 class="text-gray-600 pt-3 mb-3 mt-3 border-t-2 border-gray-300">Sunlight Exposure</h3>
            <div id="sunlightFilter" class="flex justify-between pb-3 mb-3 border-b-2 border-gray-300">
                <label for="sunlight-no-preference" class="relative cursor-pointer">
                    <input type="radio" id="sunlight-no-preference" name="sunlight" value="no-preference"
                           class="peer sr-only" checked>
